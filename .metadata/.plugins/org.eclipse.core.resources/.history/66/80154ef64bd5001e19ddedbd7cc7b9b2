package com.DItest;

public class MainApp {

	public static void main(String[] args) {

		int id;
		int salary;
		String name;
		public Employee(int id)
		{
			this.id=id;
		}
		public Employee(String name)
		{
			this.name=name;
		}
		
		public Employee(int id,String name,int salary)
		{
			this.id=id;
			this.name=name;
			this.salary=salary;
		}
		public void show()
		{
			System.out.println("ID= "+id+" "+"Name= "+name+" sal="+ salary);
		}


	}

}
