package com.demos;

import org.springframework.stereotype.Component;

@Component //Allows spring to detect our custom bean automatically, we can use this across the application to maark the beans spring managed componenent
public class Employee {
	
	private int emp_id;
	private String emp_name;
	private double emp_salary;
	
	
	public Employee() {
		
		System.out.println("Inside Default Constructor");
		
	}


	@Override
	public String toString() {
		return "Employee [emp_id=" + emp_id + ", emp_name=" + emp_name + ", emp_salary=" + emp_salary + "]";
	}
	
	
	

}
